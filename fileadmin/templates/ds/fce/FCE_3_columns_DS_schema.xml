						<numIndex index="0">outer-class</numIndex>
					</sample_data>
					<eType>input</eType>
					<tags>div:attr:class</tags>
					<proc type="array">
						<HSC type="integer">1</HSC>
					</proc>
					<TypoScript>
	10= TEXT
	10.value = cols fce-3cols row {field:field_class_outer}
	10.insertData = 1
					</TypoScript>
				</tx_templavoilaplus>
				<TCEforms type="array">
